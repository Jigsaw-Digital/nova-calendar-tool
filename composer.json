{
    "name": "czemu/nova-calendar-tool",
    "description": "Calendar tool for Laravel Nova.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0",
        "spatie/laravel-google-calendar": "^2.6"
    },
    "autoload": {
        "psr-4": {
            "Czemu\\NovaCalendarTool\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Czemu\\NovaCalendarTool\\ToolServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
